<p>
  <strong>Album:</strong>
  <%= @track.album.title %>
</p>

<p>
  <strong>Title:</strong>
  <%= @track.title %>
</p>


<% if user_signed_in? %>
	<% if @track.album.artist.user_id == current_user.id %>
		<%= link_to 'Edit', edit_track_path(@track), class: "btn btn-default"  %> |
	<% end %>
<% end %>

<%= link_to 'Back', tracks_path, class: "btn btn-default"  %>

