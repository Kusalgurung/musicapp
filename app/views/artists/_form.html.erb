<%= simple_form_for @artist, :html => { :multipart => true }  do |f| %>

  <%= select_tag(:genre_id, options_for_select(@genre), :prompt => "Select a Genre") %>
  <%= f.file_field :artist_img%>
  <%= f.input :title, label: "Artist Name: " %>
  <%= f.input :biography %>
 
  <%= f.button :submit %>

<% end %>