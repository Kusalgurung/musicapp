<%= form_for(@album) do |f| %>
    <%= f.file_field "album_img"%>
        
    <%= f.label :artist_id %>
    <p><%= @album.artist.title %></p>
    <%= f.hidden_field :artist_id, value: @album.artist.id %>
    <%= f.label :title %>
    <%= f.text_field :title %>
       
    <%= f.submit %>
<% end %>

